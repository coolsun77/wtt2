<p id="notice"><%= notice %></p>

<h1>Listing Eods</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @eods.each do |eod| %>
      <tr>
        <td><%= eod.date %></td>
        <td><%= eod.user %></td>
        <td><%= link_to 'Show', eod %></td>
        <td><%= link_to 'Edit', edit_eod_path(eod) %></td>
        <td><%= link_to 'Destroy', eod, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Eod', new_eod_path %>
