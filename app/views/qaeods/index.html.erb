<p id="notice"><%= notice %></p>

<h1>Listing Qaeods</h1>

<table>
  <thead>
    <tr>
      <th>Eod</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @qaeods.each do |qaeod| %>
      <tr>
        <td><%= link_to 'Show', qaeod %></td>
        <td><%= link_to 'Edit', edit_qaeod_path(qaeod) %></td>
        <td><%= link_to 'Destroy', qaeod, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Qaeod', new_qaeod_path %>
